    "@types/react": "^18.2.55",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "js-cookie": "^3.0.5"
  },
  "peerDependencies": {
